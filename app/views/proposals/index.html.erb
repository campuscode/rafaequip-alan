<section>
  <h1>Lista de Orçamentos</h1>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Nome</th>
        <th>Email</th>
        <th>Telefone</th>
        <th>Descrição</th>
        <th>Criado em</th>
      </tr>
    </thead>
    <tbody>
      <% @proposals.each do |proposal| %>
      <tr>
        <td><%= proposal.name %></td>
        <td><%= proposal.mail %></td>
        <td><%= proposal.phone %></td>
        <td><%= proposal.description %></td>
        <td><%= l(proposal.created_at) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</section>
<div><%= link_to 'Voltar', root_path , class: "btn btn-default" %></div>
